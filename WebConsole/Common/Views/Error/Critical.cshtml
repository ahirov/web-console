@model Exception
@{
    ViewBag.Title = "500";
}

<div class="wc-error">
    <br />
    <p class="wc-error-title">Error 500!</p>
    <br />
    <p>@Model.Message</p>
    <br/>
    <p>@Model.StackTrace</p>
</div>